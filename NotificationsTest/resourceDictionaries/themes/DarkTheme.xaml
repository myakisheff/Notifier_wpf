<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style x:Key="bgLoader" TargetType="Rectangle">
        <Setter Property="Fill" Value="#FF030303" />
    </Style>

    <Style x:Key="textLoader" TargetType="TextBlock">
        <Setter Property="Foreground" Value="#FF0D73D2" />
    </Style>

    
    
</ResourceDictionary>